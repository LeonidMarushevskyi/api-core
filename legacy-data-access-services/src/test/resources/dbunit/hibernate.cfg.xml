<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
  <session-factory>
    <property name="hibernate.current_session_context_class">thread</property>
    <property name="hibernate.dialect">org.hibernate.dialect.H2Dialect</property>
    <property name="hibernate.connection.driver_class">org.h2.Driver</property>
    <property name="show_sql">true</property>

    <mapping class="gov.ca.cwds.data.legacy.cms.entity.ChildClient" />
    <mapping class="gov.ca.cwds.data.legacy.cms.entity.Client" />
    <mapping class="gov.ca.cwds.data.legacy.cms.entity.TribalMembershipVerification" />

    
    <mapping class="gov.ca.cwds.data.legacy.cms.entity.syscodes.IndianEnrolmentStatus" />
    <mapping class="gov.ca.cwds.data.legacy.cms.entity.syscodes.IndianTribeType" />
  </session-factory>
</hibernate-configuration>
